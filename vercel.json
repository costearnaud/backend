{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        },
        {
            "src": "/api/customers",
            "dest": "/routes/api/customersController.js"
        },
        {
            "src": "/api/collections",
            "dest": "/routes/api/collectionsController.js"
        }
    ]
}